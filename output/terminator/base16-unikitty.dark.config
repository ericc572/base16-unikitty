[global_config]
  title_transmit_bg_color = "#d8137f"
  title_inactive_bg_color = "#898588"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#322a30:#d8137f:#17ad98:#dc8a0e:#735bf1:#a232dc:#149bda:#c4c2c3:#898588:#d8137f:#17ad98:#dc8a0e:#735bf1:#a232dc:#149bda:#ffffff"
    foreground_color = "#c4c2c3"
    background_color = "#322a30"
    cursor_color = "#c4c2c3"

  [[Base16 Unikitty Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#322a30:#d8137f:#17ad98:#dc8a0e:#735bf1:#a232dc:#149bda:#c4c2c3:#898588:#d8137f:#17ad98:#dc8a0e:#735bf1:#a232dc:#149bda:#ffffff"
    foreground_color = "#c4c2c3"
    background_color = "#322a30"
    cursor_color = "#c4c2c3"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Unikitty Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
